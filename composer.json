{
  "name": "flexnst/laravel-lgtv",
  "type": "library",
  "description": "LG WebOS TV control with Laravel",
  "keywords": [
    "lg",
    "tv",
    "lgtv",
    "webos",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Chernov",
      "email": "flex.nst@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
    "textalk/websocket": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Flexnst\\LgTv\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Flexnst\\LgTv\\LgTvServiceProvider"
      ],
      "aliases": {
        "LgTv": "Flexnst\\LgTv\\Facades\\LgTvFacade"
      }
    }
  }
}
